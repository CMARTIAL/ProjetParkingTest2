@model IEnumerable<ProjetParkingTest2.Models.ParkingViewModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Titre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NBPlaceTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NBPlaceLibre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Statut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coordonee0)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coordonee1)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Titre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NBPlaceTotal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NBPlaceLibre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Statut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coordonee0)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coordonee1)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

    <p>
        @Html.ActionLink("Synchro", "Synchro")
    </p>
</table>
